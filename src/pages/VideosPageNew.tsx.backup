      return success
    } catch (error) {
      console.error('åˆ é™¤IndexedDBç¼“å­˜å¤±è´¥:', error)
      return false
    }
  }, [])

  /**
   * æ£€æŸ¥ç¼©ç•¥å›¾ç¼“å­˜çŠ¶æ€ - å¢å¼ºç‰ˆæœ¬
   */
  const checkThumbnailCache = useCallback(async (video: Video) => {
    const videoId = video.id
    if (!video.thumbnail_url) {
      setThumbnailDebugInfo(prev => {
        const newMap = new Map(prev)
        newMap.set(videoId, {
          hasCachedThumbnail: false,
          isLoading: false,
          thumbnailUrl: 'No thumbnail URL',
          cacheLocation: 'None',
          isBlurImage: false, // æ— ç¼©ç•¥å›¾æ—¶ï¼Œä¸æ˜¯æ¨¡ç³Šå›¾
          remoteFileSize: '',
          cachedFileSize: '',
          cacheKey: 'no_thumbnail',
          blurImageUrl: null
        })
        return newMap
      })
      return
    }

    try {
      // è®¾ç½®åŠ è½½çŠ¶æ€
      setThumbnailDebugInfo(prev => {
        const newMap = new Map(prev)
        newMap.set(videoId, {
          hasCachedThumbnail: false,
          isLoading: true
        })
        return newMap
      })

      // æ£€æŸ¥æ˜¯å¦æœ‰ç¼“å­˜çš„ç¼©ç•¥å›¾
      const cachedThumbnail = await getCachedImage(video.thumbnail_url)
      
      // åˆ†æç¼©ç•¥å›¾ç±»å‹å’Œè´¨é‡
      let cacheType: 'base64' | 'url' | 'r2' | 'external' = 'external'
      let cacheSize = ''
      let cacheLocation = ''
      let isBlurImage = false
      let remoteUrl = video.thumbnail_url

      if (video.thumbnail_url.startsWith('data:image/svg+xml')) {
        // SVGå ä½ç¬¦
        cacheType = 'base64'
        cacheSize = 'SVGå ä½ç¬¦'
        cacheLocation = 'Browser Memory'
        isBlurImage = true
      } else if (video.thumbnail_url.startsWith('data:image/')) {
        // Base64ç¼“å­˜
        cacheType = 'base64'
        const base64Part = video.thumbnail_url.split(',')[1]
        if (base64Part) {
          const sizeKB = (base64Part.length * 0.75 / 1024).toFixed(2)
          cacheSize = `${sizeKB}KB`
          cacheLocation = 'Browser Memory (Base64)'
          // åˆ¤æ–­æ˜¯å¦ä¸ºæ¨¡ç³Šå›¾ï¼ˆå°äº15KBå¯èƒ½æ˜¯æ¨¡ç³Šå›¾ï¼‰
          isBlurImage = parseFloat(sizeKB) < 15
        }
      } else if (video.thumbnail_url.includes('cdn.veo3video.me')) {
        // R2 CDNå­˜å‚¨
        cacheType = 'r2'
        cacheLocation = 'Cloudflare R2 CDN'
        remoteUrl = video.thumbnail_url
        
        // å°è¯•æ£€æµ‹è¿œç¨‹å›¾ç‰‡å¤§å°
        try {
          const response = await fetch(video.thumbnail_url, { method: 'HEAD' })
          if (response.ok) {
            const contentLength = response.headers.get('content-length')
            if (contentLength) {
              const sizeKB = (parseInt(contentLength) / 1024).toFixed(2)
              cacheSize = `${sizeKB}KB`
              // R2å­˜å‚¨çš„å›¾ç‰‡å°äº30KBå¯èƒ½æ˜¯æ¨¡ç³Šå›¾
              isBlurImage = parseFloat(sizeKB) < 30
            } else {
              cacheSize = 'Unknown size'
              // æ— æ³•è·å–å¤§å°æ—¶ï¼Œé»˜è®¤ä¸æ˜¯æ¨¡ç³Šå›¾ï¼Œä½†è®°å½•çŠ¶æ€
              isBlurImage = false
            }
          } else {
            cacheSize = `HTTP ${response.status}`
            // HTTPé”™è¯¯æ—¶ï¼Œå¯èƒ½æ˜¯è®¿é—®é—®é¢˜ï¼Œé»˜è®¤ä¸æ˜¯æ¨¡ç³Šå›¾
            isBlurImage = false
          }
        } catch (error) {
          cacheSize = 'Check failed'
          // ç½‘ç»œé”™è¯¯æ—¶ï¼Œé»˜è®¤ä¸æ˜¯æ¨¡ç³Šå›¾
          isBlurImage = false
        }
      } else {
        // å¤–éƒ¨é“¾æ¥
        cacheType = 'external'
        cacheLocation = 'External CDN'
        remoteUrl = video.thumbnail_url
        cacheSize = 'External URL'
        // å¤–éƒ¨é“¾æ¥é»˜è®¤ä¸æ˜¯æ¨¡ç³Šå›¾ï¼ˆæ— æ³•æ£€æµ‹ï¼‰
        isBlurImage = false
      }

      // æ£€æŸ¥æ˜¯å¦æœ‰å®é™…çš„ç¼“å­˜
      const hasCachedThumbnail = !!cachedThumbnail
      
      // ç”Ÿæˆç¼“å­˜Key
      const cacheKey = video.thumbnail_url.includes('cdn.veo3video.me') ? 
        `r2_${video.id}_${video.thumbnail_url.split('/').pop()?.split('.')[0] || 'unknown'}` :
        video.thumbnail_url.startsWith('data:') ? 
        `base64_${video.id}` : 
        `url_${video.id}_${video.thumbnail_url.slice(-20)}`
      
      // æ£€æŸ¥æ˜¯å¦æœ‰æ¨¡ç³Šå›¾URLï¼ˆåŸºäºåŸURLç”Ÿæˆæ¨¡ç³Šç‰ˆæœ¬ï¼‰
      let blurImageUrl: string | null = null
      if (video.thumbnail_url.includes('cdn.veo3video.me')) {
        // å°è¯•ç”Ÿæˆæ¨¡ç³Šå›¾URLï¼ˆåœ¨æ–‡ä»¶ååæ·»åŠ -blurï¼‰
        const urlParts = video.thumbnail_url.split('.')
        if (urlParts.length > 1) {
          const extension = urlParts.pop()
          const baseName = urlParts.join('.')
          blurImageUrl = `${baseName}-blur.${extension}`
        }
      }
      
      if (cachedThumbnail && cachedThumbnail.startsWith('data:')) {
        // æœ‰Base64ç¼“å­˜çš„æƒ…å†µ
        const cachedSizeKB = (cachedThumbnail.length / 1024).toFixed(2)
        cacheLocation += ` + Cached (${cachedSizeKB}KB)`
      } else if (cachedThumbnail) {
        // URLç¼“å­˜
        cacheLocation += ' + URL Cached'
      }

      // åˆ†ç¦»è¿œç¨‹æ–‡ä»¶å¤§å°å’Œç¼“å­˜æ–‡ä»¶å¤§å°
      let remoteFileSize = ''
      let cachedFileSize = ''
      
      if (cachedThumbnail && cachedThumbnail.startsWith('data:')) {
        // æœ‰Base64ç¼“å­˜çš„æƒ…å†µï¼Œåˆ†ç¦»å¤§å°ä¿¡æ¯
        cachedFileSize = (cachedThumbnail.length / 1024).toFixed(2) + 'KB'
        remoteFileSize = cacheSize // åŸæ¥çš„cacheSizeæ˜¯è¿œç¨‹æ–‡ä»¶å¤§å°
        cacheLocation = cacheLocation.replace(/ \+ Cached \(.+?\)/, '') + ` + Cached (${cachedFileSize})`
      } else {
        remoteFileSize = cacheSize
      }

      // æŸ¥è¯¢IndexedDBä¸­çš„çœŸå®ç¼“å­˜ä¿¡æ¯
      const indexedDBCacheInfo = await queryIndexedDBCache(videoId)

      setThumbnailDebugInfo(prev => {
        const newMap = new Map(prev)
        newMap.set(videoId, {
          hasCachedThumbnail,
          cacheSize,
          cacheType,
          thumbnailUrl: video.thumbnail_url, // æ˜¾ç¤ºå®Œæ•´URL
          remoteUrl: remoteUrl, // æ˜¾ç¤ºå®Œæ•´URL
          cacheLocation,
          isBlurImage,
          isLoading: false,
          remoteFileSize, // æ–°å¢ï¼šè¿œç¨‹æ–‡ä»¶å¤§å°
          cachedFileSize, // æ–°å¢ï¼šç¼“å­˜æ–‡ä»¶å¤§å°
          cacheKey, // æ–°å¢ï¼šç¼“å­˜Key
          blurImageUrl, // æ–°å¢ï¼šæ¨¡ç³Šå›¾URL
          indexedDBCacheInfo, // æ–°å¢ï¼šIndexedDBçœŸå®ç¼“å­˜ä¿¡æ¯
        })
        return newMap
      })
    } catch (error) {
      console.error(`[VideosPageNew] æ£€æŸ¥ç¼©ç•¥å›¾ç¼“å­˜å¤±è´¥: ${videoId}`, error)
      setThumbnailDebugInfo(prev => {
        const newMap = new Map(prev)
        newMap.set(videoId, {
          hasCachedThumbnail: false,
          thumbnailUrl: 'Cache check failed',
          cacheLocation: 'Error',
          isBlurImage: false, // é”™è¯¯æ—¶é»˜è®¤ä¸æ˜¯æ¨¡ç³Šå›¾
          isLoading: false,
          remoteFileSize: '',
          cachedFileSize: '',
          cacheKey: 'error',
          blurImageUrl: null
        })
        return newMap
      })
    }
  }, [])

